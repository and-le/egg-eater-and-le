(fun (make_point x y)
    (tuple x y)
)

(fun (add_points p1 p2)
    (tuple (+ (index p1 0) (index p2 0) ) (+ (index p1 1) (index p2 1)) )
)

(let ( (p1 (make_point 5 10)) (p2 (make_point 30 60)) ) 
     (block 
        (print p1)
        (print p2)
        (print (add_points p1 p2))
     )
)
