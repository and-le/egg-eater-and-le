(fun (loopfn a b c)
    (+ (+ a b) c)
)

(
    let ( (i 0) (x 5) (y 10) (z 20) )
        (
            loop (
                if (>= i input) 
                (break i)
                (block 
                    (print (loopfn i y z))
                    (set! i (add1 i))                   
                )
            )
        )
)