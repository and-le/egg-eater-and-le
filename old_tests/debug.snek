(fun (add3 a b c)
    (let ( (x a) (y b) (z c) ) (+ (+ x y) z) )
)


(fun (mul3 a b c)
    (let ( (x a) (y b) (z c) ) (* (* x y) z) )
)

(fun (max a b)
    (if (> a b) 
        a 
        b
    )
)

(block
    (let ( (a 10) (b 20) (c 30) )
        (
            let ( (d (add3 a b c)) )
                (
                    let ( (e (mul3 a b c) ) )
                        (max d e)
                )
        )
    )
)